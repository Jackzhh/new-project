{
  "mcpServers": {
    "postgresql": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-postgresql"
      ],
      "env": {
        "POSTGRES_CONNECTION_STRING": "postgresql://mcpuser:mcppassword@localhost:5433/mcpdb"
      }
    },
    "github": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    },
    "git": {
      "command": "/Users/zhenghan/.local/bin/uvx",
      "args": [
        "mcp-server-git",
        "--repository",
        "/Users/zhenghan/CascadeProjects/new-project"
      ]
    }
  }
}
